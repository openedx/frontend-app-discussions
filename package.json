{
  "name": "@edx/frontend-app-discussions",
  "version": "0.1.0",
  "description": "Discussions Frontend",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edx/frontend-app-discussions.git"
  },
  "browserslist": [
    "last 2 versions",
    "ie 11"
  ],
  "scripts": {
    "build": "fedx-scripts webpack",
    "i18n_extract": "BABEL_ENV=i18n fedx-scripts babel src --quiet > /dev/null",
    "is-es5": "es-check es5 ./dist/*.js",
    "lint": "fedx-scripts eslint --ext .js --ext .jsx .",
    "lint:fix": "fedx-scripts eslint --ext .js --ext .jsx . --fix",
    "snapshot": "fedx-scripts jest --updateSnapshot",
    "start": "fedx-scripts webpack-dev-server --progress",
    "test": "fedx-scripts jest --coverage --passWithNoTests"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "author": "edX",
  "license": "AGPL-3.0",
  "homepage": "https://github.com/edx/frontend-app-discussions#readme",
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/edx/frontend-app-discussions/issues"
  },
  "dependencies": {
    "@edx/brand": "npm:@edx/brand-openedx@1.1.0",
    "@edx/frontend-component-footer": "10.1.4",
    "@edx/frontend-component-header": "2.2.5",
    "@edx/frontend-platform": "1.10.3",
    "@edx/paragon": "16.7.0",
    "@fortawesome/fontawesome-svg-core": "1.2.35",
    "@fortawesome/free-brands-svg-icons": "5.15.3",
    "@fortawesome/free-regular-svg-icons": "5.15.3",
    "@fortawesome/free-solid-svg-icons": "5.15.3",
    "@fortawesome/react-fontawesome": "0.1.14",
    "@reduxjs/toolkit": "1.5.1",
    "babel-polyfill": "6.26.0",
    "core-js": "3.13.1",
    "lodash.snakecase": "4.1.1",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "7.2.4",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "redux": "4.1.0",
    "regenerator-runtime": "0.13.7",
    "timeago.js": "4.0.2"
  },
  "devDependencies": {
    "@edx/frontend-build": "5.6.14",
    "codecov": "3.8.2",
    "es-check": "5.2.3",
    "glob": "7.1.7",
    "husky": "6.0.0",
    "jest": "27.0.3",
    "reactifex": "1.1.1"
  }
}
